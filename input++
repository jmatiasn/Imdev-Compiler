var notaDaProva: inteiro = 10;

main () {
    imprime("notaDaProva");
}

proc imprimirNotaDaProva() {
    imprime("notaDaProva");
}


main () {
    var novoArray: [inteiro] = [2,3,4]
    var notaDaProva: inteiro = 10;
    notaDaProva = novoArray[notaDaProva];
}


main () {
    var notaDaProva: real = V_REAL;
}


proc teste ( inteiro dez ) {
  var linguagem: string = V_STRING;

  se (verdade) entao
    linguagem = V_STRING;
  senao 
    linguagem = 10;
  fimse

}


func teste11 ( inteiro dez ) : inteiro {
  var linguagem: inteiro = V_INTEIRO;
  retorno linguagem;
}

func pegarNota(): inteiro {
    imprimirNotaDaProva();
    retorno 6;
}


func soma ( inteiro dez, inteiro vinte ) : inteiro {
  var total: inteiro = dez + vinte;

  enquanto(verdade) faca
    var total2: inteiro = dez + vinte;
  fimenquanto
  
  para(var i: inteiro = 12; 10 > total; i--) faca
    imprime("haha");
  fimpara
  
  total2++;
  var teste11: inteiro = pegarNota;
  
  retorno total;
}

func subtracao ( real dez, real vinte ) : real {
  var total: real = dez - vinte;
  retorno total;
}
